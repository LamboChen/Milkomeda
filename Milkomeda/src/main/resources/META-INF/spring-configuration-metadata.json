{
  "properties": [
    {
      "name": "milkomeda.show-log",
      "type": "java.lang.Boolean",
      "defaultValue": false,
      "description": "显示INFO级别运行日志"
    },
    {
      "name": "milkomeda.echo.pool-max-size",
      "type": "java.lang.Integer",
      "defaultValue": 200,
      "description": "连接池最大连接数"
    },
    {
      "name": "milkomeda.echo.default-max-per-route",
      "type": "java.lang.Integer",
      "defaultValue": 50,
      "description": "每个路由的并发量"
    },
    {
      "name": "milkomeda.echo.connect-timeout",
      "type": "java.lang.Integer",
      "defaultValue": 5000,
      "description": "连接超时ms"
    },
    {
      "name": "milkomeda.echo.read-timeout",
      "type": "java.lang.Integer",
      "defaultValue": 5000,
      "description": "请求读取超时ms"
    },
    {
      "name": "milkomeda.echo.connection-request-timeout",
      "type": "java.lang.Integer",
      "defaultValue": 200,
      "description": "池中获取请求连接超时ms（不宜过长）"
    },
    {
      "name": "milkomeda.echo.enable-buffer-request-body",
      "type": "java.lang.Boolean",
      "defaultValue": false,
      "description": "缓冲请求(POST/PUT)提交数据，建议不要修改，以免耗尽内存"
    },
    {
      "name": "milkomeda.echo.keep-alive",
      "type": "java.lang.Integer",
      "defaultValue": 5000,
      "description": "连接保活时长ms"
    },
    {
      "name": "milkomeda.echo.enable-request-sent-retry",
      "type": "java.lang.Boolean",
      "defaultValue": true,
      "description": "请求失败时是否允许重试"
    },
    {
      "name": "milkomeda.echo.retry-count",
      "type": "java.lang.Integer",
      "defaultValue": 3,
      "description": "请求重试次数"
    },
    {
      "name": "milkomeda.crust.stateless",
      "type": "java.lang.Boolean",
      "defaultValue": true,
      "description": "使用Toke的无状态登录"
    },
    {
      "name": "milkomeda.crust.use-rsa",
      "type": "java.lang.Boolean",
      "defaultValue": false,
      "description": "使用非对称加密签名Token"
    },
    {
      "name": "milkomeda.crust.secure-key",
      "type": "java.lang.String",
      "defaultValue": null,
      "description": "对称加密key"
    },
    {
      "name": "milkomeda.crust.pub-key",
      "type": "java.lang.String",
      "defaultValue": null,
      "description": "非对称公钥key"
    },
    {
      "name": "milkomeda.crust.pri-key",
      "type": "java.lang.String",
      "defaultValue": null,
      "description": "非对称私钥key"
    },
    {
      "name": "milkomeda.crust.expire",
      "type": "java.lang.Integer",
      "defaultValue": 30,
      "description": "Token过期时间（单位：分钟）"
    },
    {
      "name": "milkomeda.crust.token-name",
      "type": "java.lang.String",
      "defaultValue": "token",
      "description": "自定义请求Header的Token字段名"
    },
    {
      "name": "milkomeda.crust.use-bcrypt",
      "type": "java.lang.Boolean",
      "defaultValue": true,
      "description": "使用Bcrypt加密密码（将salt藏在密码里）"
    },
    {
      "name": "milkomeda.crust.enable-auto-refresh-token",
      "type": "java.lang.Boolean",
      "defaultValue": true,
      "description": "允许自动刷新Token"
    },
    {
      "name": "milkomeda.crust.refresh-token-interval",
      "type": "java.lang.Integer",
      "defaultValue": 5,
      "description": "自动刷新Token间隔（单位：分钟）"
    },
    {
      "name": "milkomeda.crust.login-url",
      "type": "java.lang.String",
      "defaultValue": "/login",
      "description": "登录路径"
    },
    {
      "name": "milkomeda.crust.logout-url",
      "type": "java.lang.String",
      "defaultValue": "/logout",
      "description": "登出路径"
    },
    {
      "name": "milkomeda.ice.enable-job-timer",
      "type": "java.lang.Boolean",
      "defaultValue": true,
      "description": "开启作业Timer（仅作为消费端使用时需要设置为false）"
    },
    {
      "name": "milkomeda.ice.delay-bucket-count",
      "type": "java.lang.Integer",
      "defaultValue": 3,
      "description": "延迟队列分桶数量"
    },
    {
      "name": "milkomeda.ice.delay-bucket-poll-rate",
      "type": "java.lang.Long",
      "defaultValue": 1000,
      "description": "延迟分桶任务轮询间隔（单位：ms）"
    },
    {
      "name": "milkomeda.ice.ttr",
      "type": "java.lang.Long",
      "defaultValue": 5000,
      "description": "任务执行超时时间（单位：ms）"
    },
    {
      "name": "milkomeda.ice.retry-count",
      "type": "java.lang.Integer",
      "defaultValue": 3,
      "description": "任务执行超时重试次数"
    },
    {
      "name": "milkomeda.ice.enable-delay-multi-retry-count",
      "type": "java.lang.Boolean",
      "defaultValue": true,
      "description": "开启任务执行超时重试时，延迟时间根据重试次数递增"
    },
    {
      "name": "milkomeda.ice.retry-delay-multi-factor",
      "type": "java.lang.Integer",
      "defaultValue": 1,
      "description": "任务执行超时重试的延迟时间增长因子"
    },
    {
      "name": "milkomeda.ice.enable-task",
      "type": "java.lang.Boolean",
      "defaultValue": false,
      "description": "使用Task处理方式接收Topic方式"
    },
    {
      "name": "milkomeda.ice.task-topic-pop-max-size",
      "type": "java.lang.Integer",
      "defaultValue": 10,
      "description": "Topic任务消费的最大个数"
    },
    {
      "name": "milkomeda.ice.task-execute-rate",
      "type": "java.lang.Long",
      "defaultValue": 5000,
      "description": "任务执行间隔（单位：ms）"
    },
    {
      "name": "milkomeda.ice.task-pop-count-lock-timeout-seconds",
      "type": "java.lang.Long",
      "defaultValue": 60,
      "description": "多个消费的并发锁超时（单位：s）"
    }
  ]
}